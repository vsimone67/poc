{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.RollingFile"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://192.168.86.182:9400"
        }
      },
      {
        "Name": "Console"
      },
      {
        "Name": "Udp",
        "Args": {
          "remoteAddress": "192.168.86.153",
          "remotePort": 7071,
          "family": "InterNetwork"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "ApplicationName": "HubService API"
    }
  },
  "Authorization": {
    "Issuer": "XXXX",
    "Audience": "XXXX",
    "TokenExpires": 60
  },
  "ServiceBusSettings": {
    "ServerName": "192.168.86.179",
    "ListenQueueName": "MibComplete",
    "SubmitQueueName": "SubmitMib",
    "NumberOfRetries": "3",
    "RetryInterval": "5000"
  },
  "Management": {
    "Endpoints": {
        "Actuator":{
            "Exposure": {
                "Include": [ "*" ]                
            }
        },
        "Metrics": {
      "Enabled": true
        }
    },
    "tracing":{
      "Name": "HubService",
      "alwaysSample": true,
      "useShortTraceIds": false,
      "exporter": {
        "zipkin": {
          "endpoint": "http://192.168.86.183:9411/api/v2/spans",
          "validateCertificates": false,
          "useShortTraceIds ": false,
          "ServiceName": "HubService"
        }
      }
     
    }
}
}
